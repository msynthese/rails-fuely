<div class="container">
  <header class="text-center">
    <h1>Favorite Locations</h1>
  </header>
  <% @locations.each do | location | %>
    <div class="flex-wrap: wrap; row location-box">
      <%# <h4></h4> %>
      <div class="location_address">
        <%= location.address %>
      </div>
      <div class="star-img text-center">
        <button class="btn rounded-4"<%= link_to location.name, "#location_path(location)" %><%= image_tag "star-images.png"%> </button>
        </div>
      </div>
    <% end %>
    <div class="text-center p-2">
      <%= link_to "New Search", locations_path, class: 'btn btn-primary rounded-4' %>
    </div>
  </div>
